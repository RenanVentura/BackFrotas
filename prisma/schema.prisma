generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Solicitacao {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  Solicitante     String
  Filial          String
  TipoServ        String
  Servico         String
  Equipamento     String
  Urgencia        String
  Descricao       String
  DataSolicitacao String
  DataEmissao     String
  Estado          String
  DataEncerrado   String
  StatusDelete    Boolean
  NumeroDoc       Int
  DateAlt         DateTime
}